<div class="modal-dialog container">
	<div class="modal-content">
		<div>
			<div class="modal-header">
				<h3 style="color: #f0ad4e;"><strong>Create Account</strong></h3>
			</div>
			<div class="modal-body">
				<form action="#" class="col-sm-12 center-block">
				<div class="input-group" style="margin-bottom: 25px;">
						<span class="input-group-addon iga1"><i class="fa fa-fw fa-user"></i></span>
						<input type="text" class="form-control input-lg"
							placeholder="First Name">
					</div>
					<div class="input-group" style="margin-bottom: 25px;">
						<span class="input-group-addon iga1"><i class="fa fa-fw fa-user"></i></span>
						<input type="text" class="form-control input-lg"
							placeholder="Last Name">
					</div>
					<div class="input-group" style="margin-bottom: 25px;">
						<span class="input-group-addon iga1"><i class="fa fa-fw fa-envelope"></i></span>
						<input type="text" class="form-control input-lg"
							placeholder="Email">
					</div>
					<div class="input-group" style="margin-bottom: 25px;">
						<span class="input-group-addon iga1"><i class="fa fa-fw fa-phone"></i></span>
						<input type="text" class="form-control input-lg"
							placeholder="Phone Number">
					</div>
					<div class="input-group" style="margin-bottom: 25px;">
						<span class="input-group-addon iga1"><i class="fa fa-fw fa-lock"></i></span>
						<input type="password" class="form-control input-lg"
							placeholder="password">
					</div>
					<div class="input-group" style="margin-bottom: 25px;">
						<span class="input-group-addon iga1"><i class="fa fa-fw fa-lock"></i></span>
						<input type="password" class="form-control input-lg"
							placeholder="re-enter password">
					</div>
					<div style="margin-top: 20px" class="form-group">
						<div class="col-sm-12 controls">
							<a id="btn-register" href="#" class="btn iga1">Create Your OneCryptor Account </a> 
						</div>
					</div>
					<div class="form-group text-center modal-footer">
						<div class="col-sm-12 control">
							<div
								style="solid #888; padding-top: 15px; font-size: 85%">
								Already have an OneCryptor Account?
								<a id="login-register" href="${pageContext.request.contextPath}/login">Sign In </a>
							</div>
							<div class="form-group">
					</div>
						</div>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>
